package com.java.oddnumbers;
import java.util.*;
public class OddNumbers {

	public static void main(String[] args) {
		int N , i , sum=0;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a number");
		N=sc.nextInt();
		for(i=7;i<=21;i=i+2) {
			
			if(i%2==i) {
				sum += i;
			}
		}
System.out.println("Sum of all odd numbers between 7 to 21 " + N + " + " + sum );
	}

}
